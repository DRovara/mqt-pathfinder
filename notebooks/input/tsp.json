{
  "settings": {
    "encoding": "ONE_HOT",
    "n_paths": 1,
    "max_path_length": 4,
    "loops": true
  },
  "objective_function": {
    "type": "MinimisePathLength",
    "path_ids": [1]
  },
  "constraints": [
    {
      "type": "PathIsValid",
      "path_ids": [1]
    },
    {
      "type": "PathContainsVerticesExactlyOnce",
      "path_ids": [1]
    }
  ]
}
