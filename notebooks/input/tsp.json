{
  "settings": {
    "encoding": "ONE_HOT",
    "n_paths": 1,
    "max_path_length": 4,
    "loops": true
  },
  "objective_function": {
    "type": "MinimisePathLength"
  },
  "constraints": [
    {
      "type": "PathIsValid"
    },
    {
      "type": "PathContainsVerticesExactlyOnce"
    }
  ]
}
